{
  "version": "0.0.0",
  "scripts": {
    "start": "vite --config build/vite.config.ts",
    "build:test": "vite --config build/vite.config.ts build --mode test",
    "build": "vue-tsc --noEmit && vite build --config build/vite.config.ts",
    "serve": "vite preview --config build/vite.config.ts",
    "cz": "cz"
  },
  "dependencies": {
    "vue": "^3.1.1",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/node": "^15.12.2",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.3.0",
    "dotenv": "^10.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vue-tsc": "^0.0.24"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-configrc.js"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
