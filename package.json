{
  "version": "0.0.0",
  "scripts": {
    "start": "vite --config build/vite.config.ts",
    "build:test": "vite --config build/vite.config.ts build --mode test",
    "build": "vue-tsc --noEmit && vite build --config build/vite.config.ts",
    "serve": "vite preview --config build/vite.config.ts",
    "lint:js": "eslint './src/**/*.{js,ts,tsx,vue,md,json}'",
    "lint:css": "stylelint ./src/**/*.{scss,vue}",
    "lint:jsfix": "nr lint:js --fix",
    "lint:cssfix": "nr lint:css --fix",
    "lint": "nr lint:js && nr lint:css",
    "lint:fix": "nr lint:jsfix && nr lint:cssfix",
    "cz": "cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.1.1",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@antfu/ni": "^0.7.0",
    "@fe6/norm": "^3.1.1",
    "@types/node": "^15.12.2",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "dotenv": "^10.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vue-tsc": "^0.0.24"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./node_modules/@fe6/norm/src/.cz-configrc.js"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,vue,md}": [
      "nr lint:js"
    ],
    "src/**/*.{scss,vue}": [
      "nr lint:css"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
